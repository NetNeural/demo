{
  "name": "netneural-iot-platform",
  "version": "3.6.0",
  "description": "NetNeural IoT Platform - Supabase-First Architecture with Next.js 15",
  "private": true,
  "author": "NetNeural Team",
  "license": "PROPRIETARY",
  "homepage": "https://netneural.github.io/SoftwareMono",
  "repository": {
    "type": "git",
    "url": "https://github.com/NetNeural/MonoRepo.git",
    "directory": "development"
  },
  "keywords": [
    "iot",
    "platform",
    "nextjs",
    "supabase",
    "typescript",
    "react",
    "golioth",
    "edge-functions"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "cross-env BUILD_MODE=dynamic next dev --turbo -H 0.0.0.0",
    "dev:debug": "cross-env BUILD_MODE=dynamic NODE_OPTIONS=--inspect next dev --turbo -H 0.0.0.0",
    "dev:full": "concurrently -n SUPA,NEXT \"npm run supabase:start\" \"sleep 5 && npm run dev\"",
    "dev:full:debug": "concurrently -n SUPA,NEXT \"npm run supabase:start\" \"sleep 5 && npm run dev:debug\"",
    "dev:functions": "concurrently -n SUPA,FUNC \"npm run supabase:start\" \"sleep 5 && npm run supabase:functions:serve\"",
    "dev:functions:debug": "concurrently -n SUPA,FUNC \"npm run supabase:start\" \"sleep 5 && supabase functions serve --inspect-brk\"",
    "dev:codespaces": "bash start-dev.sh",
    "build": "next build",
    "build:static": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:static": "npx serve@latest out",
    "export": "next build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out node_modules/.cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:extract-keys": "bash scripts/extract-supabase-keys.sh",
    "supabase:reset": "supabase db reset",
    "supabase:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "supabase:migrate": "supabase db push",
    "supabase:studio": "supabase studio",
    "supabase:functions:serve": "supabase functions serve",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:functions:logs": "supabase functions logs",
    "setup": "npm install && npm run supabase:start && wait-on http://127.0.0.1:54321/health && npm run supabase:types",
    "setup:users": "node scripts/create-test-users.js",
    "setup:dev": "bash scripts/setup-dev-db.sh",
    "setup:fresh": "npm run clean && npm install && npm run supabase:reset && npm run supabase:types",
    "health": "bash scripts/health-check.sh",
    "check": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "postinstall": "exit 0",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-iot": "^3.927.0",
    "@aws-sdk/client-iot-data-plane": "^3.927.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@sentry/nextjs": "^9.47.1",
    "@supabase/sentry-js-integration": "^0.3.0",
    "@supabase/ssr": "^0.7.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "azure-iothub": "^1.16.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.545.0",
    "mqtt": "^5.14.1",
    "next": "^15.5.5",
    "openapi-client-axios": "^7.8.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.13.2",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^2.0.3",
    "react-window": "^2.2.7",
    "recharts": "^3.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^0.1.0",
    "@next/bundle-analyzer": "^15.5.5",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.5.1",
    "@playwright/test": "^1.56.1",
    "@supabase/supabase-js": "^2.91.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.19.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^0.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.4",
    "openapi-typescript": "^6.7.6",
    "pg": "^8.18.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "supabase": "^2.45.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "wait-on": "^8.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ],
    "rules": {
      "prefer-const": "error",
      "no-var": "error",
      "@typescript-eslint/no-unused-vars": "error"
    }
  }
}
