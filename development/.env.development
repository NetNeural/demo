# Development Environment Variables for NetNeural IoT Platform
# These values are injected by GitHub Actions during dev deployment
# Local dev uses .env.local (gitignored) with local Supabase demo keys

# ============================================================================
# Application Configuration
# ============================================================================
NODE_ENV=development
NEXT_PUBLIC_APP_NAME="NetNeural IoT Platform [DEV]"
NEXT_PUBLIC_APP_VERSION="1.0.0-dev"
NEXT_PUBLIC_APP_URL="https://demo.netneural.ai"

# ============================================================================
# Supabase Configuration (Development - tsomafkalaoarnuwgdyu)
# ============================================================================
# Managed via GitHub Secrets with DEV_ prefix
# DO NOT set NEXT_PUBLIC_* variables here - they are injected by GitHub Actions
# Setting them here will override the secrets with placeholder text!
# NEXT_PUBLIC_SUPABASE_URL = set via GitHub Actions (DEV_SUPABASE_URL)
# NEXT_PUBLIC_SUPABASE_ANON_KEY = set via GitHub Actions (DEV_SUPABASE_ANON_KEY)
# SUPABASE_SERVICE_ROLE_KEY = set via GitHub Actions (DEV_SUPABASE_SERVICE_ROLE_KEY)
# SUPABASE_PROJECT_REF = set via GitHub Actions (DEV_SUPABASE_PROJECT_ID)
# SUPABASE_DB_PASSWORD = set via GitHub Actions (DEV_SUPABASE_DB_PASSWORD)

# ============================================================================
# External API Integrations (Development)
# ============================================================================
# DO NOT set API keys here - they are injected by GitHub Actions
# GOLIOTH_API_KEY = set via GitHub Actions (DEV_GOLIOTH_API_KEY)
GOLIOTH_PROJECT_ID=nn-cellular-alerts-dev
GOLIOTH_BASE_URL=https://api.golioth.io

# ============================================================================
# GitHub Deployment Configuration
# ============================================================================
# GITHUB_TOKEN = set via GitHub Actions
# SUPABASE_ACCESS_TOKEN = set via GitHub Actions

# ============================================================================
# Static Export Configuration
# ============================================================================
BUILD_MODE=static
NEXT_PUBLIC_STATIC_EXPORT=true

# ============================================================================
# Feature Flags (Development-specific)
# ============================================================================
ENABLE_DEBUG_MODE=true
ENABLE_TEST_DATA=true
DISABLE_RATE_LIMITING=true
