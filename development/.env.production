# Production Environment Variables for NetNeural IoT Platform
# These values will be set in GitHub Secrets for deployment

# ============================================================================
# Application Configuration
# ============================================================================
NODE_ENV=production
NEXT_PUBLIC_APP_NAME="NetNeural IoT Platform"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_APP_URL="https://netneural.github.io/MonoRepo"

# ============================================================================
# Supabase Configuration (Production)
# ============================================================================
# IMPORTANT: These values are managed via GitHub Secrets
# For local production testing, copy values from: gh secret list --repo NetNeural/MonoRepo
# 
# Get production values:
#   gh secret list --repo NetNeural/MonoRepo
# 
NEXT_PUBLIC_SUPABASE_URL=<from-github-secrets-NEXT_PUBLIC_SUPABASE_URL>
NEXT_PUBLIC_SUPABASE_ANON_KEY=<from-github-secrets-NEXT_PUBLIC_SUPABASE_ANON_KEY>
SUPABASE_SERVICE_ROLE_KEY=<from-github-secrets-SUPABASE_SERVICE_ROLE_KEY>
SUPABASE_PROJECT_REF=<from-github-secrets-SUPABASE_PROJECT_ID>
SUPABASE_DB_PASSWORD=<from-github-secrets-SUPABASE_DB_PASSWORD>

# ============================================================================
# External API Integrations (Production)
# ============================================================================
# IMPORTANT: Managed via GitHub Secrets
GOLIOTH_API_KEY=<from-github-secrets-GOLIOTH_API_KEY>
GOLIOTH_PROJECT_ID=nn-cellular-alerts
GOLIOTH_BASE_URL=https://api.golioth.io

# OpenAI API Key (for AI-powered predictive insights)
# IMPORTANT: Managed via GitHub Secrets
OPENAI_API_KEY=<from-github-secrets-OPENAI_API_KEY>

# ============================================================================
# GitHub Deployment Configuration
# ============================================================================
# IMPORTANT: Managed via GitHub Secrets
GITHUB_TOKEN=<from-github-secrets-GITHUB_TOKEN>
SUPABASE_ACCESS_TOKEN=<from-github-secrets-SUPABASE_ACCESS_TOKEN>

# ============================================================================
# Static Export Configuration
# ============================================================================
BUILD_MODE=static
NEXT_PUBLIC_STATIC_EXPORT=true